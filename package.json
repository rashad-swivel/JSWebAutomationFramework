{
  "name": "webdriverio-cucumber-pom-boilerplate",
  "version": "1.0.0",
  "description": "A WebdriverIO & Cucumber Boilerplate based on Page Object Model",
  "scripts": {
    "open-Allure": "./node_modules/.bin/allure generate ./Reports/allure-results --clean && allure open",
    "test": "npx wdio wdio.conf.js --parallel=false",
    "test-dev": "set NODE_ENV=dev && npx wdio wdio.conf.js --parallel=false",
    "test-uat1": "set NODE_ENV=uat1 && npx wdio wdio.conf.js --parallel=false"
  },
  "keywords": [
    "webdriverio",
    "cucumber",
    "page object model",
    "pom"
