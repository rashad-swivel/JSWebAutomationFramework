    "@wdio/local-runner": "^7.19.5",
    "@wdio/selenium-standalone-service": "^6.10.4",
    "@wdio/spec-reporter": "^7.19.5",
    "@wdio/sync": "^7.19.5",
    "chromedriver": "^96.0.0",
